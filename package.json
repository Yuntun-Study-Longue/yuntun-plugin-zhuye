{
  "name": "yuntun-plugin-zhuye",
  "homepage": "https://images.yuntun-bj.com",
  "version": "0.0.27",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">= 14.15"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "rescripts eject",
    "serve": "cross-env NODE_ENV=production node ./server/index.js",
    "extract": "formatjs extract -- 'src/**/*js' --out-file src/app/lang/extra/zh.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile": "formatjs compile -- src/app/lang/extra/zh.json --ast --out-file src/app/lang/compiled/zh.json --format formatter.js",
    "es5": "babel handler.js -o index.js && babel src -d dist/src",
    "oss": "ossutilmac64 cp -r ./src/app/assets oss://yuntun-web/yuntun_web/static/media --update && ossutilmac64 cp -r ./build/static oss://yuntun-web/$npm_package_name/$npm_package_version/static --update && ossutilmac64 cp -r ./build oss://yuntun-web/$npm_package_name/$npm_package_version --exclude static --update",
    "assets": "cpy 'src/**/*.jpg' 'dist/' --parents && cpy 'src/**/*.png' 'dist/' --parents && cpy 'src/**/*.css' 'dist/' --parents",
    "publish:full": "cross-env PUBLIC_URL=$npm_package_homepage/$npm_package_name/$npm_package_version npm run build && npm run es5 && npm run assets && npm publish && npm run oss",
    "publish:simple": "npm publish",
    "publish:local": "cross-env PUBLIC_URL=$npm_package_homepage/$npm_package_name/$npm_package_version npm run build && npm run es5 && npm run assets && npm run oss",
    "preinstall": "npm cache clean --force",
    "prepublish:local": "npm link luna-utils wechat-jssdk"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {}
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@formatjs/cli": "^3.1.3",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-macros": "^2.5.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.5.0",
    "cookie-parser": "^1.4.3",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "forcedomain": "^1.0.0",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^1.1.0",
    "ignore-styles": "^5.0.1",
    "js-cookie": "^2.2.0",
    "md5-file": "^4.0.0",
    "morgan": "^1.9.1",
    "prettier": "1.14.3",
    "pretty-quick": "^1.7.0",
    "query-string": "^6.14.0",
    "react": "^16.5.2",
    "react-scripts": "^2.0.4",
    "react-dom": "^16.5.2",
    "react-dynamic-swiper": "^2.0.0",
    "react-frontload": "^1.0.3",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.8.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.2",
    "superagent": "^6.1.0"
  },
  "dependencies": {
    "@rescripts/cli": "0.0.16",
    "axios": "^0.26.1",
    "i": "^0.3.7",
    "luna-utils": "^1.0.11",
    "marked": "^1.2.7",
    "npm": "^8.6.0",
    "rc-form": "^2.4.11",
    "rc-message": "^1.3.7",
    "react-grid-layout": "^1.2.0",
    "react-is": "^17.0.1",
    "react-lazy-load-image-component": "^1.5.1",
    "react-select": "^3.1.1",
    "styled-theming": "^2.2.0",
    "yuntun-wechat-jssdk": "^5.0.7"
  }
}
